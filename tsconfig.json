{
  "compilerOptions": {
    "lib": [
      "DOM",
      "ES2017",
      "WebWorker"
    ],
    "target": "ES2017",
    "module": "ES2020",
    "moduleResolution": "node",
    "sourceMap": true,
    "allowSyntheticDefaultImports": true,
    "useDefineForClassFields": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "strictNullChecks": true,
    "paths": {
      "@services/*": [
        "./src/services/*"
      ],
      "@assets": [
        "./src/assets"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@mixins/*": [
        "./src/mixins/*"
      ],
      "@native": [
        "./src/native",
        "./src/native/*"
      ],
      "@types": [
        "./src/types"
      ]
    }
  },
  "vueCompilerOptions": {
    "target": 2.7
  },
  "ts-node": {
    "compilerOptions": {
      "module": "CommonJS"
    }
  }
}